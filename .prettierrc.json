{
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "vueIndentScriptAndStyle": false,
  "htmlWhitespaceSensitivity": "css",
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "rangeStart": 0,
  "printWidth": 150
}
